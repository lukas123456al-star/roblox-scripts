SliderGlow.Thickness = 2
SliderGlow.Color = Color3.fromRGB(255,0,0)

-- // BUTTONS // --
local function MakeButton(txt, posX)
	local B = Instance.new("TextButton")
	B.Size = UDim2.new(0.5,-15,0,28)
	B.Position = UDim2.new(posX, posX == 0 and 10 or -10, 0, 65)
	B.BackgroundColor3 = Color3.fromRGB(20,20,20)
	B.TextColor3 = Color3.fromRGB(255,255,255)
	B.Text = txt
	B.BorderSizePixel = 0
	B.Parent = Frame
	Instance.new("UICorner", B).CornerRadius = UDim.new(0,8)

	-- Hover glow
	B.MouseEnter:Connect(function()
		TweenService:Create(B, TweenInfo.new(.2), {BackgroundColor3 = Color3.fromRGB(60,0,0)}):Play()
	end)
	B.MouseLeave:Connect(function()
		TweenService:Create(B, TweenInfo.new(.2), {BackgroundColor3 = Color3.fromRGB(20,20,20)}):Play()
	end)

	return B
end

local Plus5 = MakeButton("+5",0)
local Minus5 = MakeButton("-5",0.5)

-- // LOGIC // --
local char = player.Character or player.CharacterAdded:Wait()
local humanoid = char:WaitForChild("Humanoid")

local minVal = 0
local maxVal = 200

local function SetJump(v)
	v = math.clamp(v,minVal,maxVal)
	humanoid.JumpPower = v
	InputBox.Text = tostring(v)
	SliderBtn.Position = UDim2.new(v/maxVal,-7,-0.2,0)
end

-- DRAG SLIDER
local dragging = false

SliderBack.InputBegan:Connect(function(i)
	if i.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
	end
end)

UIS.InputEnded:Connect(function(i)
	if i.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
	end
end)

UIS.InputChanged:Connect(function(i)
	if dragging then
		local percent = math.clamp((i.Position.X - SliderBack.AbsolutePosition.X) / SliderBack.AbsoluteSize.X, 0,1)
		SetJump(percent * maxVal)

		-- animate glow
		SliderGlow.Thickness = 4
		TweenService:Create(SliderGlow,TweenInfo.new(.2),{Thickness = 2}):Play()
	end
end)


-- BUTTONS
Plus5.MouseButton1Click:Connect(function()
	SetJump(humanoid.JumpPower + 5)
end)

Minus5.MouseButton1Click:Connect(function()
	SetJump(humanoid.JumpPower - 5)
end)

-- TEXTBOX INPUT
InputBox.FocusLost:Connect(function()
	local n = tonumber(InputBox.Text)
	if n then SetJump(n) end
end)

-- TOGGLE MENU WITH R
local Visible = true
UIS.InputBegan:Connect(function(i)
	if i.KeyCode == Enum.KeyCode.R then
		Visible = not Visible
		TweenService:Create(Frame, TweenInfo.new(.25), {
			BackgroundTransparency = Visible and 0.2 or 1
		}):Play()
		Frame.Visible = Visible
	end
end)

SetJump(50)














